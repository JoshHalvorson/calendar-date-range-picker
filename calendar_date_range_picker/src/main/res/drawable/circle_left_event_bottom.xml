<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@+id/background" android:top="8dp" android:bottom="8dp" android:end="25dp">
        <shape
            android:innerRadius="0dp"
            android:shape="rectangle"
            android:thicknessRatio="3"
            android:useLevel="false">
            <solid android:color="@color/blue_four" />
        </shape>
    </item>
    <item android:id="@+id/circle" android:top="5dp" android:bottom="5dp">
        <shape
            android:innerRadius="0dp"
            android:shape="ring"
            android:thicknessRatio="3"
            android:useLevel="false">
            <solid  android:color="@color/blue_one" />
            <stroke
                android:width="1.5dp"
                android:color="@color/blue_one" />
        </shape>
    </item>

    <item
        android:id="@+id/eventDot"
        android:end="5dp"
        android:start="5dp"
        android:top="25dp">
        <shape
            android:innerRadius="0dp"
            android:shape="ring"
            android:thicknessRatio="16"
            android:useLevel="false">
            <solid android:color="@color/blue_one" />
            <stroke
                android:width="1.5dp"
                android:color="@color/blue_one" />
        </shape>
    </item>
</layer-list>

